{
  "name": "store-ratings",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "watch": "watch 'npm run build' . --wait=10 --ignoreDotFiles",
    "build": "npm run clean && npm run build:css && npm run build:js",
    "build:css": "node-sass ./src/sass/style.scss --output-style compressed ./dist/style/style.css | postcss --use autoprefixer --map --replace ./dist/style/style.css",
    "build:js": "browserify ./src/javascripts/main.js --outfile  --transform [ babelify --presets [ 'es2015'] ] --debug | exorcist ./dist/javascripts/bundle.map.js > ./dist/javascripts/bundle.js && uglifyjs ./dist/javascripts/bundle.js -c -m -o ./dist/javascripts/bundle.js",
    "clean": "rm -r -f dist/javascripts/** dist/style/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TruJared/store-ratings.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TruJared/store-ratings/issues"
  },
  "homepage": "https://github.com/TruJared/store-ratings#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.0",
    "connect": "^3.6.6",
    "cors-anywhere": "^0.4.1",
    "netlify-lambda": "^0.4.0",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "exorcist": "^1.0.1",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.21",
    "postcss-cli": "^5.0.0",
    "uglify-js": "^3.3.22",
    "uglifyjs": "^2.4.11",
    "watch": "^1.0.2"
  }
}
